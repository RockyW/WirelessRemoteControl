<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <ImageView
        android:id="@+id/id_symbol_equal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_equal"
        android:src="@drawable/bg_symbol_equal" />

    <ImageView
        android:id="@+id/id_symbol_comma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_comma"
        android:src="@drawable/bg_symbol_comma" />

    <ImageView
        android:id="@+id/id_symbol_colon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_colon"
        android:src="@drawable/bg_symbol_colon" />

    <ImageView
        android:id="@+id/id_symbol_single_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_single_quote"
        android:src="@drawable/bg_symbol_single_quote" />

    <ImageView
        android:id="@+id/id_symbol_semicolon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_semicolon"
        android:src="@drawable/bg_symbol_semicolon" />

    <ImageView
        android:id="@+id/id_symbol_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_quote"
        android:src="@drawable/bg_symbol_quote" />

    <ImageView
        android:id="@+id/id_symbol_tilde"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_tilde"
        android:src="@drawable/bg_symbol_tilde" />

    <ImageView
        android:id="@+id/id_symbol_vertical_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_vertical_line"
        android:src="@drawable/bg_symbol_vertical_line" />

    <ImageView
        android:id="@+id/id_symbol_backslash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_backslash"
        android:src="@drawable/bg_symbol_backslash" />

    <ImageView
        android:id="@+id/id_symbol_underline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:contentDescription="@string/symbol_underline"
        android:src="@drawable/bg_symbol_underline" />

</merge>